    <script>

        document.addEventListener('DOMContentLoaded', function() {
        const maxQuantity = 5; // Maximum quantity that can be ordered
        const availableStock = <%= variant.quantity %>; // Get available stock from backend

        console.log('This is my availablestock');

        const quantityInput = document.getElementById('quantity');
        const stockMessageDiv = document.getElementById('stockMessage');
        const cartForm = document.getElementById('cartForm');

        // Set max attribute for the quantity input field
        quantityInput.max = Math.min(maxQuantity, availableStock);

        // Update stock message based on available stock
        function updateStockMessage(quantity) {
            if (availableStock === 0) {
                stockMessageDiv.textContent = 'Out of stock';
            } else if (quantity > availableStock) {
                stockMessageDiv.textContent = `Only ${availableStock} in stock`;
            } else {
                stockMessageDiv.textContent = '';
            }
        }

        // Initial stock message update
        updateStockMessage(quantityInput.value);

        // Listen for changes in the quantity input field
        quantityInput.addEventListener('input', function() {
            const quantity = parseInt(quantityInput.value, 10);
            if (quantity > maxQuantity) {
                quantityInput.value = maxQuantity;
            }
            updateStockMessage(quantity);
        });

        // Handle form submission
